[Unit]
Description=Mita proxy server
After=network-online.target network.service networking.service NetworkManager.service systemd-networkd.service
Wants=network-online.target
StartLimitBurst=5
StartLimitIntervalSec=60

[Service]
Type=exec
User=root
Group=root
Environment="MITA_CONFIG_JSON_FILE=/root/mfolder/config.json"
Environment="MITA_LOG_NO_TIMESTAMP=true"
ExecStart=/root/mfolder/mita run
Nice=-10
Restart=on-failure
RestartSec=5

[Install]
WantedBy=multi-user.target
