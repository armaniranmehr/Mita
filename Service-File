[Unit]
Description=Mieru proxy client
After=network-online.target network.service networking.service NetworkManager.service systemd-networkd.service
Wants=network-online.target
StartLimitBurst=5
StartLimitIntervalSec=60

[Service]
Type=exec
Environment="MIERU_CONFIG_JSON_FILE=/root/mfolder/config.json"
ExecStart=/root/mfolder/mieru run
Restart=on-failure
RestartSec=1

[Install]
WantedBy=multi-user.target
